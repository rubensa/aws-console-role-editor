{
  "manifest_version": 3,
  "name": "AWS Console Role Editor",
  "description": "This extension allows you to modify your AWS Console Roles.",
  "version": "1.0.0",
  "icons": {
    "128": "icons/128x128-aws-console.png",
    "16": "icons/16x16-aws-console.png"
  },
  "action": {
    "default_icon": "icons/16x16-aws-console.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies"
  ],
  "host_permissions": [
    "https://console.aws.amazon.com/"
  ]
}